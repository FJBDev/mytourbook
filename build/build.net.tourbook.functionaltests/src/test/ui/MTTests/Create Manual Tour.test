------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-button "Create new tour/event (Ctrl+N)" | click
get-view "Tour Editor" | get-section "Tour / Event" | get-combo -after [get-label Title] | set-text NewTour
get-button "Save modified tour (M1+S)" | click
with [get-view "Tour Editor" | get-section "Tour / Event" | get-combo -after [get-label Title]] {
    get-property "isVisible()" | equals true | verify-true
    get-property "toString()" | equals "Combo {NewTour} [layout=null]" | verify-true
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
