   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
         
      <application>
         <run
               class="net.tourbook.application.Application">
         </run>
      </application>
      
   </extension>
   
  
   
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
         
      <product
            application="net.tourbook.application"
            name="MyTourbook">
            
         <property
               name="windowImages"
               value="icons/application/tourbook16.gif,icons/application/tourbook32.gif,icons/application/tourbook48.gif,icons/application/tourbook64.gif,icons/application/tourbook128.gif"/>
         <property
               name="aboutText"
               value="%Product_about"/>
         <property
               name="aboutImage"
               value="icons/application/tourbook-about-250.gif"/>
         <property
               name="appName"
               value="MyTourbook">
         </property>
         <property
               name="startupForegroundColor"
               value="BBCBFF">
         </property>
         <property
               name="startupMessageRect"
               value="5,116,390,15">
         </property>
         <property
               name="startupProgressRect"
               value="5,131,390,10">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
      
   </extension>
   
   
   <extension
         point="org.eclipse.core.runtime.preferences">
